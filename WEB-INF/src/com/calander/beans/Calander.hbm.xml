<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.calander.beans.Calander" table="dbo.db_calander">
        <id name="case_id" column="case_id">
            <generator class="native"/>
        </id>
        <property name="search_date" column="search_date"/>
        <property name="case_no" column="case_no"/>
        <property name="heading_status" column="heading_status"/>
        <property name="judge1" column="judge1"/>
        <property name="judge2" column="judge2"/>
        <property name="judge3" column="judge3"/>
        <property name="lcourt" column="lcourt"/>
        <property name="venue" column="venue"/>
        <property name="case_ref" column="case_ref"/>
        <property name="title1" column="title1"/>
        <property name="title2" column="title2"/>
        <property name="type" column="type"/>
        <property name="lc_judge" column="lc_judge"/>
        <property name="nature" column="nature"/>
        <property name="last_updated" column="last_updated"/>
        <property name="result" column="result"/>
        <property name="status" column="status"/>
        <property name="track_line1" column="track_line1"/>
        <property name="track_line2" column="track_line2"/>
        <property name="track_line3" column="track_line3"/>
        <property name="track_line4" column="track_line4"/>
        <property name="track_line5" column="track_line5"/>
        <property name="track_line6" column="track_line6"/>
        <property name="track_line7" column="track_line7"/>
        <property name="track_line8" column="track_line8"/>
    </class>
</hibernate-mapping>